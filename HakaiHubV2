--Load
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/hakaiofc/Base/main/HUBORION')))()

--Main Script
local Window = OrionLib:MakeWindow({Name = "ùêáùêÄùêäùêÄùêà_ùêéùêÖùêÇ", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest", IntroEnabled = false})

--Valor
_G.AutoHealth = True
--Fun√ß√£o
function AutoHealth()
while _G.AutoHealth == true do
local args = {
    [1] = "GiveHealthCuzPro",
    [2] = 2,
    [3] = 3
}
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))

game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
local args = {
    [1] = "GiveHealthCuzPro",
    [2] = 2,
    [3] = 3
}
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
local args = {
    [1] = "GiveHealthCuzPro",
    [2] = 2,
    [3] = 3
}
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
local args = {
    [1] = "GiveHealthCuzPro",
    [2] = 2,
    [3] = 3
}
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
game:GetService("ReplicatedStorage"):WaitForChild("andreymax"):FireServer(unpack(args))
wait(HAKAI_OFC)
end
end

