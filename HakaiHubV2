--Main
local FarmTab= Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})
local Section = FarmTab:AddSection({
    Name = "AutoFarm"
})
FarmTab:AddToggle({
    Name = "Auto-Health",
    Default = false,
    Callback = function(Value)
        _G.AutoHealth = Value
        AutoHealth(HAKAI_OFC)
    end
})
FarmTab:AddToggle({
    Name = "Auto-Health2",
    Default = false,
    Callback = function(Value)
        _G.AutoVida = Value
        AutoVida(HAKAI_OFC)
    end
})
FarmTab:AddToggle({
    Name = "Auto-Health3",
    Default = false,
    Callback = function(Value)
        _G.AutoVD = Value
